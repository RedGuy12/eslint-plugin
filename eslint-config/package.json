{
	"name": "@redguy12/eslint-config",
	"version": "1.1.0",
	"description": "My ESLint Style Guide",
	"keywords": [
		"eslint",
		"eslint-plugin",
		"eslint-config"
	],
	"homepage": "https://github.com/redguy12/configs/tree/main/eslint-config#readme",
	"bugs": {
		"url": "https://github.com/redguy12/configs/issues",
		"email": "paul@reid-family.org"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/RedGuy12/configs.git"
	},
	"license": "MIT",
	"author": "Paul Reid",
	"exports": {
		".": "./src/recommended.js",
		"./browser": "./src/browser.js",
		"./cli": "./src/cli.js",
		"./config": "./src/config.js",
		"./esm": "./src/esm.js",
		"./node": "./src/node.js",
		"./recommended": "./src/recommended.js",
		"./sample": "./src/sample.js",
		"./typescript": "./src/typescript.js",
		"./vue": "./src/vue.js"
	},
	"main": "src/recommended.js",
	"scripts": {
		"lint": "npx eslint . --fix",
		"format": "prettier --write \"**\" --ignore-unknown --cache",
		"types": "npx tsc -p jsconfig.json"
	},
	"prettier": "@redguy12/prettier-config",
	"dependencies": {
		"@redguy12/eslint-plugin": "file:../eslint-plugin",
		"@shopify/eslint-plugin": "42.0.2",
		"@typescript-eslint/eslint-plugin": "5.46.1",
		"eslint-config-hardcore": "26.0.0",
